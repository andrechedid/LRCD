<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="800.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lrcdepartment.HistoryController">
   <children>
      <Pane prefHeight="80.0" prefWidth="800.0" style="-fx-background-color: #E22121;">
         <children>
            <MenuButton fx:id="searchCondition" layoutX="555.0" layoutY="28.0" mnemonicParsing="false" style="-fx-border-radius: 70; -fx-background-radius: 70;" text="SearchCondition">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#updSCM" text="Missions" />
                <MenuItem mnemonicParsing="false" onAction="#uSCD" text="Donations" />
              </items>
            </MenuButton>
            <ImageView fitHeight="44.0" fitWidth="52.0" layoutX="690.0" layoutY="25.0" onMouseClicked="#search" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/search.png" />
               </image></ImageView>
            <ImageView fitHeight="50.0" fitWidth="61.0" layoutX="36.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/invImg.png" />
               </image></ImageView>
         </children>
      </Pane>
      <Button layoutX="27.0" layoutY="444.0" mnemonicParsing="false" onAction="#back" prefHeight="25.0" prefWidth="61.0" style="-fx-background-color: #6A8099; -fx-border-color: WHITE; -fx-border-radius: 50; -fx-background-radius: 50;" text="Back" textFill="WHITE" />
      <TextArea fx:id="dmInfo" editable="false" layoutX="38.0" layoutY="150.0" prefHeight="258.0" prefWidth="711.0" style="-fx-border-color: #38423C; -fx-background-radius: 100; -fx-border-radius: 10; -fx-border-width: 10;" />
   </children>
</AnchorPane>
